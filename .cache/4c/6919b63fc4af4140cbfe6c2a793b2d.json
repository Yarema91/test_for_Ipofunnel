}\n});\n\nstore.dispatch({\n    type: 'EDIT_SHOP',\n    payload: {\n        id: '62',\n        itemName: 'ReplaceName',\n        count: 'ReplaceCount'\n    }\n})\n\nconsole.log(store.getState());\n\n\n\n"},"lineCount":null}},"error":null,"hash":"f4dac76e1541f48b2f9b39c32ae026c3","cacheData":{"env":{}}}